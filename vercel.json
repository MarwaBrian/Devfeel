{
  "builds": [
    {
      "src": "blog/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "static/**",
      "use": "@vercel/static"
    },
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blog/wsgi.py"
    }
  ]
}
